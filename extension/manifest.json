{
	"manifest_version": 2,
	
	"name": "Trakkit",
	"author": "Garrett Downs",
	"description": "Super simple and fast package tracker app.",
	"version": "0.0.1",
	"icons": {
		"256": "icon-256.png",
		"128": "icon-128.png",
		"64": "icon-64.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"24": "icon-24.png"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
	    }
	},
	"permissions" : [
		"<all_urls>",
		"storage"
	]
}